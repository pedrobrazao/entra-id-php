{
    "require": {
        "php": ">=8.2",
        "slim/slim": "^4.14",
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^3.8",
        "guzzlehttp/psr7": "^2.7",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "^7.2",
        "friendsofphp/php-cs-fixer": "^3.75",
        "rector/rector": "^2.0"
    }
}
