{% extends "base.html.twig" %}

{% block title %}List Users{% endblock %}

{% block content %}
<h2>Users ({{ users|length }})</h2>
{% if users|length > 0 %}
<table>
<thead>
<tr>
<th>Name</th>
<th>Email</th>
</tr>
</thead>
<tbody>
{% for user in users %}
<tr>
<td>
<a href="{{ url_for(constant('App\\Handler\\Admin\\UserGetHandler::NAME'), {id: user.id}) }}">
{{ user.displayName }}
</a>
</td>
<td>{{ user.mail }}</td>
</tr>
{% endfor %}
</tbody>
</table>
{% endif %}
{% endblock %}
