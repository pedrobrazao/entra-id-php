{% extends "base.html.twig" %}

{% block title %}{{ application.displayName }}{% endblock %}

{% block content %}
<h2>{{ application.displayName }}</h2>
<dl>
<dt>ID:</dt>
<dd>{{ application.id }}</dd>
<dt>Created At:</dt>
<dd>{{ application.createdDateTime|date('c') }}</dd>
{% if application.appId %}
<dt>App ID:</dt>
<dd>{{ application.appId }}</dd>
{% endif %}
{% if application.publisherDomain %}
<dt>Publisher Domain:</dt>
<dd>{{ application.publisherDomain }}</dd>
{% endif %}
</dl>
{% endblock %}
